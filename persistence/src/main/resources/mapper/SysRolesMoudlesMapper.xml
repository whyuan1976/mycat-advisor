<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"
        "http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">
<mapper namespace="org.apache.mycat.advisor.persistence.dao.SysRolesMoudlesMapper">

	<select id="findModelIdsByRoleId" resultType="Long">
	<![CDATA[
		SELECT MODULE_ID FROM sys_roles_moudles where  role_id =#{roleId};
    ]]>
	</select>

</mapper>